public class MyClass {
    public static void main(String args[]) {
    for (int row = 1; row<=4; row++){
        if (row ==1 || row==4)
            System.out.println("$$$$");
        else
            System.out.println("$  $");
    }
    System.out.println("");
    }
    }
    
    
    OUTPUT:
    $$$$
    $  $
    $  $
    $$$$
